{
  "name": "jira-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "lint-fix": "eslint --fix --ext .js,.ts,.tsx .",
    "ts-verify": "tsc",
    "generate:api": "openapi-typescript https://developer.atlassian.com/cloud/jira/platform/swagger-v3.v3.json --output src/server/jiraClient/generatedTypes.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --max-warnings=0 --fix"
    ]
  },
  "dependencies": {
    "@trpc/client": "^9.27.2",
    "@trpc/next": "^9.27.2",
    "@trpc/react": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "next": "12.3.1",
    "openapi-typescript-fetch": "^1.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.2",
    "superjson": "^1.10.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "openapi-typescript": "^5.4.1",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  },
  "ct3aMetadata": {
    "initVersion": "5.13.1"
  }
}
